{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:has_component",
    "component": "minecraft:custom_name",
    "on_true": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when": "Candy bag",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/bundles/halloween/spider_goodie_bag"
          }
        },
        {
          "when": "Small Red Present",
          "model": {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:bundle/fullness",
            "scale": 64,
            "entries": [
              {
                "threshold": 0,
                "model": {
                  "type": "minecraft:model",
                  "model": "item/deco/christmas/presents/small/red_gift_empty"
                }
              },
              {
                "threshold": 1,
                "model": {
                  "type": "minecraft:model",
                  "model": "item/deco/christmas/presents/small/red_gift"
                }
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/red_bundle"
            }
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/red_bundle"
      }
    },
    "on_false": {
      "type": "minecraft:model",
      "model": "minecraft:item/red_bundle"
    }
  }
}