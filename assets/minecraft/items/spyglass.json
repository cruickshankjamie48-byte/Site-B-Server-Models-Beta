{
  "oversized_in_gui": true,
  "model": {
    "type": "select",
    "component": "custom_name",
    "cases": [
      {
        "when": "Cigar",
        "model": {
          "type": "minecraft:model",
          "model": "item/smokes/sigar",
          "tints": []
        }
      },
      {
        "when": "Pipe",
        "model": {
          "type": "minecraft:model",
          "model": "item/smokes/trubka"
        }
      },
      {
        "when": "Ekima Cigar",
        "model": {
          "type": "minecraft:model",
          "model": "item/smokes/sigar_ekima"
        }
      }
    ],
    "fallback": {
      "type": "select",
      "property": "display_context",
      "cases": [
        {
          "when": [
            "gui",
            "ground",
            "fixed"
          ],
          "model": {
            "type": "model",
            "model": "minecraft:item/spyglass"
          }
        }
      ],
      "fallback": {
        "type": "condition",
        "property": "using_item",
        "on_true": {
          "type": "minecraft:model",
          "model": "minecraft:item/spyglass_in_hand",
          "base": "minecraft:item/spyglass_in_hand"
        },
        "on_false": {
          "type": "minecraft:model",
          "model": "minecraft:item/spyglass_in_hand",
          "base": "minecraft:item/spyglass_in_hand"
        }
      }
    },
    "property": "component"
  }
}